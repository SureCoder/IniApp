<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

        <style name="CustomTabLayoutText" parent="TextAppearance.AppCompat.Widget.ActionBar.Title.Inverse">
            <item name="android:textSize">30sp</item>
            <item name="android:textAllCaps">true</item>
        </style>
    <style name="AppCompatAlertDialogStyle" parent="Theme.AppCompat.Light.Dialog.Alert">
        <!--对话框按钮文字颜色-->
        <item name="colorAccent">#424242</item>
        <!--对话框内容文字颜色-->
        <item name="android:textSize">17sp</item>
        <!--标题字体颜色-->
        <item name="android:textColorPrimary">#454545</item>
        <!--对话框背景色-->
        <item name="android:background">#fafafa</item>
    </style>
</resources>
